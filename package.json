{
  "name": "superfarma-tools",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm run format:check && npm run typecheck",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "npx tsx prisma/seed-production.ts",
    "db:reset": "prisma migrate reset --force && prisma migrate deploy && npm run db:seed",
    "db:studio": "prisma studio",
    "services:up": "docker compose -f prisma/docker/compose.yml up -d",
    "services:stop": "docker compose -f prisma/docker/compose.yml stop",
    "services:down": "docker compose -f prisma/docker/compose.yml down",
    "setup:prod": "prisma migrate deploy && npm run db:seed:prod"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-router/node": "^7.8.0",
    "@react-router/serve": "^7.8.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "currency.js": "^2.0.4",
    "date-fns": "^4.1.0",
    "html-to-image": "^1.11.13",
    "isbot": "^5.1.27",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.536.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-router": "^7.8.0",
    "recharts": "^3.1.2",
    "remix-auth": "^4.2.0",
    "remix-hook-form": "^7.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@react-router/dev": "^7.8.0",
    "@tailwindcss/vite": "^4.1.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/recharts": "^1.8.29",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.13.0",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
